{
  "Version": "2012-10-17",
  "Statement": [
    {
      "Sid": "ddb",
      "Effect": "Allow",
      "Action": [
          "dynamodb:Batch*",
          "dynamodb:DeleteItem",
          "dynamodb:GetItem",
          "dynamodb:PutItem",
          "dynamodb:Scan",
          "dynamodb:UpdateItem"
      ],
      "Resource": [
        "arn:aws:dynamodb:%AWS_REGION%:%AWS_ACCOUNT%:table/Bookings"
      ]
    }
  ]
}
